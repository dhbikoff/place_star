GET     /assets/*file                       controllers.Assets.versioned(path="/public", file: Asset)

GET     /                                   controllers.Assets.at(path="/public", file="index.html")
GET     /ping                               controllers.Application.ping
GET     /google/:search/:width/:height      controllers.Application.google(search: String, width: Int, height: Int)
GET     /bing/:search/:width/:height        controllers.Application.bing(search: String, width: Int, height: Int)
